{
    "buildCommand": "",
    "outputDirectory": ".",
    "rewrites": [
        {
            "source": "/admin",
            "destination": "/admin/login.html"
        }
    ],
    "headers": [
        {
            "source": "/(.*)\\.css",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "text/css"
                }
            ]
        },
        {
            "source": "/(.*)\\.js",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "application/javascript"
                }
            ]
        }
    ]
}